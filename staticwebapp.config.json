{
  "platform": {
    "apiRuntime": "node:18"
  },
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin_ui/index.html",
      "headers": {
        "Cache-Control": "no-store",
        "X-Robots-Tag": "noindex, nofollow"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}
