{
  "platform": {
    "apiRuntime": "node:18"
  },
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin_ui/index.html",
      "headers": {
        "Cache-Control": "no-store",
        "X-Robots-Tag": "noindex, nofollow"
      }
    },
    {
      "route": "/CardSense_BGRemover_Demo.exe",
      "headers": {
        "Content-Type": "application/octet-stream",
        "Content-Disposition": "attachment; filename=\"CardSense_BGRemover_Demo.exe\""
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}
